{
    "name": "smb/tplus",
    "description": "Tplus - Laravel package for generating QR codes, vCards and more",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Smb\\Tplus\\": "src/"
        }
    },
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0 || ^12.0",
        "simplesoftwareio/simple-qrcode": "^4.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Smb\\Tplus\\QrCodeServiceProvider"
            ],
            "aliases": {
                "QrCode": "Smb\\Tplus\\Facades\\QrCode"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
